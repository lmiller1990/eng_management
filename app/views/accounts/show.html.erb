<%= form_with(
     model: @rubric_evaluation,
     url: account_rubric_evaluations_path(account_id: @account.id),
   ) do |form| %>

  <%= form.select :rubric_id,
              options_from_collection_for_select(@rubrics, :id, :name),
              include_blank: true %>

  <%= form.submit class: "btn btn-primary btn-xs" %>

<% end %>
